{
  "name": "emifdapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "e2e": "ng e2e",
    "build.prod": "npm run build.prod.i18n",
    "build.prod.en": "npm run build.prod.en.dist && npm run build.prod.en.manifest",
    "build.prod.en.dist": "ng build --configuration=prod-en",
    "build.prod.en.manifest.desc": "build.prod.en.manifest  replaces 'start_url = / to start_url= /es  just to increase Audits percetange",
    "build.prod.en.manifest": "node ./node_modules/replace-in-file/bin/cli.js  \"start_url\\\": \\\"/\\\"\" \"start_url\\\": \\\"/en/index.html\\\"\" ./dist/en/manifest.webmanifest ",
    "build.prod.es": "npm run build.prod.es.dist && npm run build.prod.es.manifest",
    "build.prod.es.dist": "ng build --configuration=prod-es",
    "build.prod.es.manifest.desc": "echo replaces 'start_url = / to start_url= /es ",
    "build.prod.es.manifest": "node ./node_modules/replace-in-file/bin/cli.js  \"start_url\\\": \\\"/\\\"\" \"start_url\\\": \\\"/es/index.html\\\"\" ./dist/es/manifest.webmanifest ",
    "build.prod.i18n": "npm run build.prod.en && npm run build.prod.es",
    "build.prod.copy.webconfig": "copy \"./web.config\" \"./dist/web.config\" ",
    "serve.prod": "ng serve --prod",
    "serve.prod.es": "ng serve --aot --i18n-file=src/i18n/messages.es.xlf  --locale=es  --i18n-format=xlf",
    "serve.prod.lite-server": "npm run build.prod.i18n && lite-server --baseDir=\"./dist\" ",
    "lint": "ng lint",
    "lint.fix": "ng lint emifdapp --fix",
    "lint.prod": "ng lint",
    "test": "ng test --watch --code-coverage",
    "test.mocha": "ng test --reporters=mocha --watch",
    "test.prod": "ng test --watch=false --code-coverage --browsers=ChromeHeadlessCustom --reporters=coverage-istanbul ",
    "test.code-coverage": "ng test --code-coverage",
    "i18n": "ng xi18n --output-path src/i18n "
  },
  "private": true,
  "dependencies": {
    "@amcharts/amcharts4": "4.9.0",
    "@angular/animations": "9.0.0",
    "@angular/cdk": "9.0.0",
    "@angular/common": "9.0.0",
    "@angular/compiler": "9.0.0",
    "@angular/core": "9.0.0",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/forms": "9.0.0",
    "@angular/localize": "^9.0.0",
    "@angular/material": "9.0.0",
    "@angular/platform-browser": "9.0.0",
    "@angular/platform-browser-dynamic": "9.0.0",
    "@angular/pwa": "0.900.1",
    "@angular/router": "9.0.0",
    "@angular/service-worker": "9.0.0",
    "@ngrx/effects": "8.6.0",
    "@ngrx/entity": "8.6.0",
    "@ngrx/store": "8.6.0",
    "@ngrx/store-devtools": "8.6.0",
    "@progress/kendo-angular-charts": "^4.1.3",
    "@progress/kendo-angular-common": "^1.2.2",
    "@progress/kendo-angular-intl": "^2.0.1",
    "@progress/kendo-angular-l10n": "^2.0.1",
    "@progress/kendo-angular-popup": "^3.0.5",
    "@progress/kendo-drawing": "^1.6.0",
    "bootstrap": "4.4.1",
