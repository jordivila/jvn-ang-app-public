    "karma-jasmine-html-reporter": "1.4.2",
    "karma-mocha-reporter": "^2.2.5",
    "protractor": "5.4.3",
    "replace-in-file": "5.0.2",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.7.5"
  }
}
